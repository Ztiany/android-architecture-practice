<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="InsetCompatLayout">
        <attr name="iclDirection">
            <flag name="top" value="0x01" />
            <flag name="bottom" value="0x02" />
            <flag name="left" value="0x04" />
            <flag name="right" value="0x08" />
        </attr>
        <attr name="iclType">
            <flag name="statusBar" value="0x01" />
            <flag name="navigationBar" value="0x02" />
            <flag name="captionBar" value="0x04" />
            <flag name="gesture" value="0x08" />
            <flag name="cutout" value="0x10" />
            <flag name="systemBars" value="0x07" />
        </attr>
        <attr name="iclLeftInsetColor" format="color|reference" />
        <attr name="iclTopInsetColor" format="color|reference" />
        <attr name="iclRightInsetColor" format="color|reference" />
        <attr name="iclBottomInsetColor" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="InsetsCompatFrameLayout">
        <attr name="iclDirection" />
        <attr name="iclType" />
        <attr name="iclLeftInsetColor" />
        <attr name="iclTopInsetColor" />
        <attr name="iclRightInsetColor" />
        <attr name="iclBottomInsetColor" />
    </declare-styleable>

    <declare-styleable name="InsetsCompatLinearLayout">
        <attr name="iclDirection" />
        <attr name="iclType" />
        <attr name="iclLeftInsetColor" />
        <attr name="iclTopInsetColor" />
        <attr name="iclRightInsetColor" />
        <attr name="iclBottomInsetColor" />
    </declare-styleable>

    <declare-styleable name="InsetsCompatConstraintLayout">
        <attr name="iclDirection" />
        <attr name="iclType" />
        <attr name="iclLeftInsetColor" />
        <attr name="iclTopInsetColor" />
        <attr name="iclRightInsetColor" />
        <attr name="iclBottomInsetColor" />
    </declare-styleable>

</resources>